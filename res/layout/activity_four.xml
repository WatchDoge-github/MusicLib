<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Four"
    android:scrollbars="vertical">

  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_height="40dp"
      android:id="@+id/RL1">
    <TextView
        android:id="@+id/Setting1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="40dp"
        android:text="Interface"
        android:textSize="20dp" />

    <View
        android:id="@+id/line1"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignParentBottom="true"
        android:background="@color/white" />

  </RelativeLayout>


  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_height="40dp"
      app:layout_constraintTop_toBottomOf="@+id/RL1"
      android:id="@+id/RL2">

    <TextView
        android:id="@+id/Setting2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="40dp"
        android:text="Sound"
        android:textSize="20dp" />

    <View
        android:id="@+id/line2"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignParentBottom="true"
        android:background="@color/white" />

  </RelativeLayout>


  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_height="40dp"
      app:layout_constraintTop_toBottomOf="@+id/RL2"
      android:id="@+id/RL3">

    <TextView
        android:id="@+id/Setting3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="40dp"
        android:text="Sound Effects"
        android:textSize="20dp" />

    <View
        android:id="@+id/line3"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignParentBottom="true"
        android:background="@color/white" />

  </RelativeLayout>


  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_height="40dp"
      app:layout_constraintTop_toBottomOf="@+id/RL3"
      android:id="@+id/RL4">

    <TextView
        android:id="@+id/Setting4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="40dp"
        android:text="Adding the files"
        android:textSize="20dp" />

    <View
        android:id="@+id/line4"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignParentBottom="true"
        android:background="@color/white" />

  </RelativeLayout>


  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_height="40dp"
      app:layout_constraintTop_toBottomOf="@+id/RL4"
      android:id="@+id/RL5">

    <TextView
        android:id="@+id/Setting5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="40dp"
        android:text="Bookmarks"
        android:textSize="20dp" />

    <View
        android:id="@+id/line5"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignParentBottom="true"
        android:background="@color/white" />

  </RelativeLayout>


  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_height="40dp"
      app:layout_constraintTop_toBottomOf="@+id/RL5"
      android:id="@+id/RL6">

    <TextView
        android:id="@+id/Setting6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="40dp"
        android:text="@string/hyperlink1"
        android:textSize="20dp" />

    <View
        android:id="@+id/line6"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignParentBottom="true"
        android:background="@color/white" />

  </RelativeLayout>


  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_height="40dp"
      app:layout_constraintTop_toBottomOf="@+id/RL6"
      android:id="@+id/RL7">

    <TextView
        android:id="@+id/Setting7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="40dp"
        android:text="@string/hyperlink2"
        android:textSize="20dp" />

    <View
        android:id="@+id/line7"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignParentBottom="true"
        android:background="@color/white" />

  </RelativeLayout>

  </androidx.constraintlayout.widget.ConstraintLayout>